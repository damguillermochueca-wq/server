<odoo>
    <record id="action_alumno" model="ir.actions.act_window">
        <field name="name">Alumnos</field>
        <field name="res_model">fpinformatica.alumno</field>
        <field name="view_mode">tree,form</field>
        <field name="search_view_id" ref="view_alumno_search"/>
    </record>

    <record id="action_empresa" model="ir.actions.act_window">
        <field name="name">Empresas</field>
        <field name="res_model">fpinformatica.empresa</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_root" name="FCT" web_icon="fpinformatica,static/description/icon.png"/>
    
    <menuitem id="menu_alumnos" parent="menu_root" action="action_alumno" sequence="10"/>
    <menuitem id="menu_empresas" parent="menu_root" action="action_empresa" sequence="20"/>
</odoo>